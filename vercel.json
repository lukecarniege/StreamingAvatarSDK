{
  "version": 2,
  "builds": [
    { "src": "src/api/**/*.js", "use": "@vercel/node" },
    { "src": "src/pages/api/**/*.js", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "pages/api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
